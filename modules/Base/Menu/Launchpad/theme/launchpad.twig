<div id="launchpadModal" class="modal fade" aria-hidden="true" style="display: none; z-index: 1050">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" data-dismiss="modal" aria-hidden="true" class="close">Ã—</button>
                <h4 class="modal-title">Launchpad</h4>
            </div>
            <div class="modal-body">
                {% for item in icons %}
                    <a {{ item.url | raw}} class="btn btn-default epe-btn" data-dismiss="modal">
                        {% if item.icon is not null %}
                            <i class="fa fa-{{ item.icon }} fa-3x"></i>
                        {% endif %}
                        <div>{{ item.label }}</div>
                    </a>
                {% endfor %}
            </div>
            <div class="modal-footer">
                <button type="button" data-dismiss="modal" class="btn btn-default">Close</button>
            </div>
        </div>
    </div>
</div>